#include <iostream>

using namespace std;

int main()
{
    int a,b,c;
    cout<<"Ingrese el valor del primer entero ";
    cin>>a;
    cout<<"Ingrese el valor del segundo entero ";
    cin>>b;
    
    c=a;
    a=(a*0)+b;
    b=(b*0)+c;
    
    cout<<"El nuevo valor de a es "<<a<<endl;
    cout<<"El nuevo valor de b es "<<b<<endl;

    return 0;
}
